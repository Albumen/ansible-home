[push]
	default = simple
[user]
	name =
	email =
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[core]
	excludesfile = ~/.gitignore_global
	editor = "atom --wait"
  pager = less
	autocrlf = input
[help]
  autocorrect = 20
[color]
  ui = auto
[credential]
  helper = osxkeychain
[alias]
	lol = log --oneline --graph --decorate --all
	po = push origin
	pom = push origin master
	poh = push origin HEAD
